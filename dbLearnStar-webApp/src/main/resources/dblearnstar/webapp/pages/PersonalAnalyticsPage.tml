<html t:type="layout" title="message:personalanalytics-page"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
	xmlns:p="tapestry:parameter">

<div class="row">
	<div class="col-lg-12">
		<h1>${message:PersonalAnalytics-pagelink}</h1>
	</div>
</div>

<div class="row">
	<div class="col-lg-12">
		<h2>${message:ranking-label}</h2>
		<table class="table table-hover table-bordered table-striped">
			<tr>
				<th>${message:userName-label}</th>
				<th>${message:count-label}</th>
			</tr>
			<tr t:type="loop" t:source="studentsSolving" t:value="personCount">
				<td>${name}</td>
				<td>${count}</td>
			</tr>
		</table>
	</div>
</div>

</html>
